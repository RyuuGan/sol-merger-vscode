{
	"name": "sol-merger",
	"displayName": "Solidity Contracts Merger",
	"description": "Merge solidity contracts with one command",
	"version": "0.0.1",
	"publisher": "RyuuGan",
	"repository": "https://github.com/RyuuGan/sol-merger-vscode",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onCommand:extension.sol-merger.merge"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.sol-merger.merge",
				"title": "Sol-merger"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/node": "^8.10.25",
		"tslint": "^5.11.0",
		"typescript": "^2.6.1",
		"vscode": "^1.1.22"
	},
	"dependencies": {
		"node-glob": "^1.2.0",
		"sol-merger": "^0.1.3"
	}
}
